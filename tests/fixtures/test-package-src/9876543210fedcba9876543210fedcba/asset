using UnityEngine;

namespace TestPackage.Safe
{
    /// <summary>
    /// 安全なゲームロジックのスクリプト（検出されるべきでない）
    /// </summary>
    public class SafeGameplayScript : MonoBehaviour
    {
        [SerializeField] private float moveSpeed = 5.0f;
        [SerializeField] private GameObject playerPrefab;
        
        void Start()
        {
            Debug.Log("Player initialized successfully");
        }
        
        void Update()
        {
            if (Input.GetKeyDown(KeyCode.Space))
            {
                Jump();
            }
        }
        
        void Jump()
        {
            GetComponent<Rigidbody2D>()?.AddForce(Vector2.up * 10f, ForceMode2D.Impulse);
        }
    }
}